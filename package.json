{
  "name": "erth",
  "version": "0.1.0",
  "description": "A concatenative actor language",
  "main": "index.js",
  "scripts": {
    "test": "node src/test.js | node_modules/.bin/faucet"
  },
  "author": "Justin Falcone <justin@justinfalcone.com>",
  "license": "MIT",
  "dependencies": {
    "pegjs": "^0.9.0",
    "redux-action-schema": "^0.7.3"
  },
  "devDependencies": {
    "eslint": "^3.0.1",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-standard": "^1.3.2",
    "faucet": "0.0.1",
    "tape": "^4.6.0"
},
"eslintConfig": {
    "extends": "standard",
    "rules": {
      "quotes": [
        2,
        "double",
        {
          "avoidEscape": true,
          "allowTemplateLiterals": true
        }
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "indent": [
        2,
        4
      ]
    }
  }
}
